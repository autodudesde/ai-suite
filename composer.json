{
    "name": "autodudes/ai-suite",
    "type": "typo3-cms-extension",
    "description": "The AI Suite optimizes the workflow of project managers, agencies and freelancers by using the latest AI technologies. It seamlessly integrates a wide variety of AI interfaces and the AI Suite open source models into the TYPO3 backend. Enables, among other things, more efficient creation and management of image and page metadata (individually and as batch processing), content translations (including \"Easy Language\"), content creation and modification, image generation and page structure generation.",
    "authors": [
        {
            "name": "AutoDudes",
            "role": "Developer",
            "homepage": "https://www.autodudes.de",
            "email": "service@autodudes.de"
        }
    ],
    "homepage": "https://www.autodudes.de",
    "license": [
        "GPL-2.0-or-later"
    ],
    "keywords": [
        "TYPO3 CMS",
        "TYPO3 AI",
        "Artificial Intelligence",
        "Content Generation",
        "Content Creation",
        "Content Automation",
        "AutoDudes",
        "AI Suite"
    ],
    "version": "12.11.0",
    "support": {
        "issues": "https://github.com/autodudesde/ai-suite/issues"
    },
    "require": {
        "php": "^8.1",
        "typo3/cms-backend": "^12.4.11",
        "typo3/cms-filelist": "^12.4.11",
        "typo3/cms-fluid": "^12.4.11",
        "typo3/cms-rte-ckeditor": "^12.4.11",
        "typo3/cms-seo": "^12.4.11"
    },
    "suggest": {
        "b13/container": "*"
    },
    "conflict": {
        "passionweb/ai-seo-helper": "*"
    },
    "autoload": {
        "psr-4": {
            "AutoDudes\\AiSuite\\": "Classes/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true,
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "b13/container": "^2.3",
        "bk2k/bootstrap-package": "^14.0",
        "friendsofphp/php-cs-fixer": "^3.60",
        "georgringer/news": "^11.4",
        "helhum/typo3-console": "^8.1",
        "phpstan/phpstan": "^1.10",
        "typo3/cms-beuser": "^12.4.11",
        "typo3/cms-lowlevel": "^12.4.11",
        "typo3/cms-tstemplate": "^12.4.11",
        "typo3/cms-install": "^12.4.11",
        "typo3/cms-impexp": "^12.4.11",
        "typo3/cms-indexed-search": "^12.4.11",
        "typo3/cms-filemetadata": "^12.4.11",
        "typo3/cms-form": "^12.4.11",
        "typo3/coding-standards": "^0.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "typo3/cms": {
            "extension-key": "ai_suite",
            "web-dir": ".Build/Web"
        }
    }
}
