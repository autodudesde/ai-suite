{
    "name": "autodudes/ai-suite",
    "type": "typo3-cms-extension",
    "description": "Adds a backend module as well as various helper functionalities and features to your TYPO3 backend, which are intended to make the daily work of editors easier and faster with the help of AI.",
    "authors": [
        {
            "name": "AutoDudes",
            "role": "Developer",
            "homepage": "https://www.autodudes.de",
            "email": "service@autodudes.de"
        }
    ],
    "homepage": "https://www.autodudes.de",
    "license": [
        "GPL-2.0-or-later"
    ],
    "keywords": [
        "TYPO3 CMS",
        "TYPO3 AI",
        "Artificial Intelligence",
        "Content Generation",
        "Content Creation",
        "Content Automation",
        "AutoDudes",
        "AI Suite"
    ],
    "version": "13.0.1",
    "support": {
        "issues": "https://github.com/autodudesde/ai-suite/issues"
    },
    "require": {
        "php": "^8.2",
        "typo3/cms-backend": "^13.4",
        "typo3/cms-filelist": "^13.4",
        "typo3/cms-fluid": "^13.4",
        "typo3/cms-seo": "^13.4",
        "typo3/cms-rte-ckeditor": "^13.4"
    },
    "suggest": {
        "b13/container": "*"
    },
    "conflict": {
        "passionweb/ai-seo-helper": "*",
        "dmitryd/dd-deepl": "^12.0",
        "web-vision/wv_deepltranslate": "^4.3",
        "pits/ai_translate": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "AutoDudes\\AiSuite\\": "Classes/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true,
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "contentblocks/content-blocks": "^1.0",
        "bk2k/bootstrap-package": "^15.0",
        "georgringer/news": "^12.0",
        "b13/container": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.52",
        "helhum/typo3-console": "^8.2",
        "phpstan/phpstan": "^1.10",
        "typo3/cms-beuser": "^13.4",
        "typo3/cms-lowlevel": "^13.4",
        "typo3/cms-tstemplate": "^13.4",
        "typo3/cms-install": "^13.4",
        "typo3/coding-standards": "^0.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "typo3/cms": {
            "extension-key": "ai_suite",
            "web-dir": ".Build/Web"
        }
    }
}
